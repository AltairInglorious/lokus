{
  "name": "lokus",
  "type": "module",
  "version": "0.3.12",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist/"
  ],
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./*": {
      "import": "./dist/*.js",
      "require": "./dist/*.cjs"
    },
    "./generators": {
      "import": "./dist/generators/index.js",
      "require": "./dist/generators/index.cjs"
    }
  },
  "license": "MIT",
  "scripts": {
    "prepublishOnly": "bunx tsc",
    "build": "bunx tsc"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/AltairInglorious/lokus.git"
  },
  "description": "Lokus is a library for i18n purposes to manage dictionaries. Currently WIP",
  "homepage": "https://lokus.amadeustech.dev/",
  "bugs": {
    "url": "https://github.com/AltairInglorious/lokus/issues"
  },
  "author": {
    "name": "Adam",
    "url": "https://github.com/AltairInglorious"
  },
  "keywords": [
    "i18n",
    "lokus",
    "localization",
    "internationalization",
    "translation"
  ],
  "dependencies": {
    "zod": "^3.25.32"
  },
  "peerDependencies": {
    "typescript": "^5"
  }
}